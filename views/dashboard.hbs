{{> menu id="dashboard"}}

{{#each assessments}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <table class="ui table">
      <thead>
        <tr>
          <th>Weight</th>
          <th>Chest</th>
            <th>Thigh</th>
            <th>Upper Arm</th>
            <th>Waist</th>
            <th>Hips</th>
        </tr>
      </thead>
      <tbody>
        {{#each songs}}
          <tr>
            <td>
              {{Weight}}
            </td>
            <td>
              {{Chest}}
            </td>
              <td>
                  {{Thigh}}
              </td>
              <td>
                  {{UpperArm}}
              </td>
              <td>
                  {{Waist}}
              </td>
              <td>
                  {{Hips}}
              </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </section>
{{/each}}